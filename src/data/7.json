{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "description": "A simple bar chart with embedded data.",
    "width": 800,
    "data": {"url":"https://raw.githubusercontent.com/chloet127/info474-a2/main/src/data/genreCount.csv"},
    "mark": "line",
    "encoding": {
      "x": {"field": "year", "type": "ordinal"},
      "y": {"field": "num_songs", "type": "quantitative"},
      "tooltip": {"field": "num_songs", "type": "quantitative"},
      "color": {"field": "playlist_genre", "type": "nominal"}
    }
  }